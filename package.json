{
    "name": "wucdbm-vue-errors",
    "private": false,
    "description": "A Vue.js bindings library for wucdbm/filter-bundle PHP toolbox",
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "dev": "vite",
        "preview": "vite preview",
        "build": "tsc --p ./tsconfig-build.json && vite build",
        "prepublishOnly": "npm run build",
        "lint": "eslint .",
        "format": "prettier --write .",
        "prepare": "husky install",
        "test": "vitest --reporter verbose"
    },
    "peerDependencies": {
        "vue": "^3.4.15",
        "axios": "^1.6.7"
    },
    "peerDependenciesMeta": {
        "vue": {
            "optional": false
        },
        "axios": {
            "optional": false
        }
    },
    "devDependencies": {
        "@commitlint/cli": "^18.5.0",
        "@commitlint/config-conventional": "^18.5.0",
        "@rollup/plugin-replace": "^5.0.5",
        "@types/node": "^20.11.6",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.4",
        "typescript": "^5.2.2",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.2",
        "vitest": "^1.2.2"
    },
    "lint-staged": {
        "**/*.{js,ts,tsx}": [
            "eslint --fix"
        ],
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "vue": "^3.4.15",
        "axios": "^1.6.7"
    }
}
